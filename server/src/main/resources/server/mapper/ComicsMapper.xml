<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="server.mapper.ComicsMapper">
    <select id="getComicsById" resultType="server.model.Comics">
        SELECT * FROM COMICS WHERE COMICS_ID = #{id}
    </select>
</mapper>
